{
  "name": "rely-wallet",
  "version": "1.1.0",
  "description": "Rely is a wallet & web3 gateway for all blockchain ecosystems",
  "license": "LGPL-3.0-or-later",
  "author": "CraftLabs Tech <hello@craftlabs.tech>",
  "homepage": "https://getrely.io",
  "repository": {
    "type": "git",
    "url": "https://github.com/craftlabs-tech/rely-wallet.git"
  },
  "scripts": {
    "android": "react-native run-android",
    "android:clean": "cd android && ./gradlew clean && cd ..",
    "android:release": "react-native run-android --variant=release",
    "android:active-arch": "react-native run-android --active-arch-only",
    "android:bundle": "react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/src/main/res",
    "ios": "react-native run-ios",
    "ios:bundle": "react-native bundle --platform ios --dev false --entry-file index.js --bundle-output ios/main.jsbundle --assets-dest ios",
    "xcode": "xed -b ios",
    "compile": "graphql-codegen",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "start": "react-native start",
    "start:cache": "react-native start --reset-cache",
    "start:dev": "react-native rnx-start",
    "start:dev:cache": "react-native rnx-start --reset-cache",
    "bundle": "react-native rnx-bundle --dev false",
    "bundle:ios": "react-native rnx-bundle --dev false --platform ios",
    "bundle:android": "react-native rnx-bundle --dev false --platform android",
    "bundle:windows": "react-native rnx-bundle --dev false --platform windows",
    "bundle:macos": "react-native rnx-bundle --dev false --platform macos",
    "typecheck": "tsc",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:report": "jest --collectCoverage --coverageDirectory=\"./coverage\" --ci --reporters=default --reporters=jest-junit --coverage",
    "pod:deintegrate": "bundle exec pod deintegrate --project-directory=ios",
    "pod:install": "RCT_NEW_ARCH_ENABLED=0 bundle exec pod install --project-directory=ios --repo-update",
    "pod-install:new-arch": "RCT_NEW_ARCH_ENABLED=1 bundle exec pod install --project-directory=ios --repo-update",
    "pod-install:prod": "PRODUCTION=1 RCT_NEW_ARCH_ENABLED=0 bundle exec pod install --project-directory=ios --repo-update",
    "codegen": "graphql-codegen --config codegen.ts",
    "patch": "patch-package"
  },
  "rnx-kit": {
    "kitType": "app",
    "bundle": [
      {
        "id": "main",
        "entryFile": "index.js",
        "assetsDest": "dist",
        "targets": [
          "android",
          "ios",
          "macos",
          "windows"
        ],
        "plugins": [
          [
            "@rnx-kit/metro-plugin-cyclic-dependencies-detector",
            {
              "throwOnError": false
            }
          ],
          [
            "@rnx-kit/metro-plugin-duplicates-checker",
            {
              "throwOnError": false
            }
          ]
        ],
        "platforms": {
          "android": {
            "bundleOutput": "dist/index.android.bundle",
            "sourcemapOutput": "dist/index.android.bundle.map",
            "assetsDest": "dist/res"
          },
          "ios": {
            "bundleOutput": "dist/index.ios.jsbundle",
            "sourcemapOutput": "dist/index.ios.jsbundle.map"
          },
          "macos": {
            "bundleOutput": "dist/index.macos.jsbundle",
            "sourcemapOutput": "dist/index.macos.jsbundle.map"
          },
          "windows": {
            "bundleOutput": "dist/index.windows.bundle",
            "sourcemapOutput": "dist/index.windows.bundle.map"
          }
        }
      }
    ]
  },
